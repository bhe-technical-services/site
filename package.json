{
  "name": "bts",
  "description": "BHE Technical services' website",
  "version": "1.0.0",
  "author": "GT Fari <admin@goosebumps.co.zw>",
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "2.24.58",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-manifest": "2.4.27",
    "gatsby-plugin-offline": "3.2.26",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.31",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-source-graphql": "^2.7.5",
    "gatsby-transformer-sharp": "^2.5.15",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "^2.0.8",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -p 8080",
    "cleanDevelop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8080",
    "deploy": "node ./hooks/build.js",
    "dev": "npm run develop",
    "devc": "npm run cleanDevelop",
    "serve": "gatsby serve"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.6.2",
    "@tailwindui/react": "^0.1.1",
    "autoprefixer": "9.8.6",
    "axios": "^0.20.0",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.8.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "gatsby-plugin-eslint": "2.0.8",
    "tailwindcss": "1.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/txndai/bts"
  },
  "bugs": {
    "url": "https://github.com/txndai/bts/issues"
  }
}